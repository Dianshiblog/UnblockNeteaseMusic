{
    "extends": [
        "airbnb"
    ],
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "no-restricted-syntax": 0,
      "func-names": 0,
      "no-param-reassign": 0,
      "no-console": 0,
      "no-mixed-operators": 0,
      "no-continue": 0,
      "no-await-in-loop": 0
    }
}
